{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": 1,
        "no-undef": 1,
        "prettier/prettier": [
            "error",
            {
              "singleQuote": true,
              "parser": "flow",
              "endOfLine": "auto"
            }
          ]
    }
}
